{
  "name": "@bvite/vfui",
  "version": "1.0.2-beta.5",
  "description": "vfui vue3 component library",
  "main": "dist/vfui.cjs.js",
  "module": "dist/vfui.es.js",
  "types": "es/index.d.ts",
  "style": "dist/index.css",
  "unpkg": "dist/vfui.umd.js",
  "keywords": [
    "vfui",
    "vue3",
    "vite"
  ],
  "author": "feasin",
  "files": [
    "dist",
    "theme-chalk",
    "es",
    "lib"
  ],
  ".": {
    "import": "./dist/vfui.es.js",
    "require": "./dist/vfui.cjs.js",
    "./es": "./es/index.mjs",
    "./lib": "./lib/index.js"
  },
  "sideEffects": [
    "dist/*",
    "theme-chalk/*.css",
    "theme-chalk/src/*.less",
    "es/components/*/style/*",
    "lib/components/*/style/*",
    "es/components/*/src/**",
    "lib/components/*src/**"
  ],
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "license": "MIT"
}
